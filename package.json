{
  "name": "fe",
  "description": "Grunt plugin to generate grunt init templates",
  "version": "0.1.3",
  "homepage": "https://leecade.github.com/fe",
  "author": {
    "name": "leecade",
    "email": "leecade@163.com",
    "url": "https://github.com/leecade"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/leecade/fe.git"
  },
  "bugs": {
    "url": "https://github.com/leecade/fe/issues"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "execa": "^0.4.0",
    "listr": "^0.4.1",
    "ora": "^0.2.3",
    "progress": "^1.1.8",
    "semver": "^5.2.0",
    "updtr": "^0.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-fe": "^1.0.11",
    "espower": "^1.3.2",
    "json-loader": "^0.5.4",
    "power-assert": "^1.4.1",
    "pre-commit": "^1.1.3",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "webpack": "^2.1.0-beta.15"
  },
  "keywords": [
    "fe"
  ],
  "engines": {
    "npm" : "3.x"
  },
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "fe": "./lib/index.js"
  },
  "scripts": {
    "dev": "webpack --watch",
    "start": "npm run dev",
    "prepublish": "npm version patch -m \"bump %s\"",
    "precommit": "git diff --name-only --cached --relative | grep '\\.jsx\\?$' | xargs standard | snazzy; if [ $? -ne 0 ]; then exit 1; fi"
  },
  "pre-commit": {
    "run": [
      "precommit"
    ],
    "silent": true
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "fetch"
    ],
    "ignore": [
      "bin/",
      "lib/",
      "mock/",
      "node_modules/"
    ]
  },
  "license": "MIT"
}
