  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "fe": "./bin/fe.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "babel:build": "babel src --out-dir lib",
    "babel:watch": "babel src --out-dir lib --watch",
    "watch": "gulp watch",
    "dev": "npm run build & npm run watch",
    "build": "gulp",
    "start": "npm run build",
    "release": "./scripts/release.sh",
    "update": "updtr",
    "prepublish": "npm run build && npm version patch -m \"bump version\"",
    "precommit": "git diff --name-only --cached --relative | grep '\\.jsx\\?$' | xargs standard | snazzy; if [ $? -ne 0 ]; then exit 1; fi"
  },
  "pre-commit": {
    "run": [
      "precommit"
    ],
    "silent": true
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "fetch",
      "alert"
    ],
    "ignore": [
      "bin/",
      "lib/",
      "mock/",
      "src/plugins/",
      "example/dist/",
      "node_modules/"
    ]
  },
